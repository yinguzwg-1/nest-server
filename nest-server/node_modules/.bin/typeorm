#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/zwg/Desktop/demo/nest-server/nest-server/node_modules/.pnpm/typeorm@0.3.24_mysql2@3.14._caaeab751e65fbec5abd7ed7ba59268e/node_modules/typeorm/node_modules:/mnt/c/Users/zwg/Desktop/demo/nest-server/nest-server/node_modules/.pnpm/typeorm@0.3.24_mysql2@3.14._caaeab751e65fbec5abd7ed7ba59268e/node_modules:/mnt/c/Users/zwg/Desktop/demo/nest-server/nest-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/zwg/Desktop/demo/nest-server/nest-server/node_modules/.pnpm/typeorm@0.3.24_mysql2@3.14._caaeab751e65fbec5abd7ed7ba59268e/node_modules/typeorm/node_modules:/mnt/c/Users/zwg/Desktop/demo/nest-server/nest-server/node_modules/.pnpm/typeorm@0.3.24_mysql2@3.14._caaeab751e65fbec5abd7ed7ba59268e/node_modules:/mnt/c/Users/zwg/Desktop/demo/nest-server/nest-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
